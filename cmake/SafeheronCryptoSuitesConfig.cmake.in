@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
find_dependency(Protobuf REQUIRED)
find_dependency(OpenSSL REQUIRED)

include("${CMAKE_CURRENT_LIST_DIR}/SafeheronCryptoSuitesTargets.cmake")

message(STATUS "SafeheronCryptoSuites: Loading package in compatibility mode.")
set_and_check(SafeheronCryptoSuites_INCLUDE_DIR "${PACKAGE_PREFIX_DIR}/include")
set_and_check(SafeheronCryptoSuites_INCLUDE_DIRS "${PACKAGE_PREFIX_DIR}/include")
set_and_check(SafeheronCryptoSuites_LIBRARY_DIR "${PACKAGE_PREFIX_DIR}/lib")
get_target_property(SafeheronCryptoSuites_LIBRARY
                    SafeheronCryptoSuites
                    IMPORTED_LOCATION)
if(NOT SafeheronCryptoSuites_LIBRARY)
    get_target_property(SafeheronCryptoSuites_LIBRARY
                        SafeheronCryptoSuites
                        IMPORTED_LOCATION_RELEASE)
endif()

if(NOT SafeheronCryptoSuites_LIBRARY)
    message(FATAL_ERROR "Could not determine library location for target SafeheronCryptoSuites. The installation may be broken.")
else()
    set_and_check(SafeheronCryptoSuites_LIBRARY "${SafeheronCryptoSuites_LIBRARY}")
    message(STATUS "Found SafeheronCryptoSuites at: ${SafeheronCryptoSuites_LIBRARY}")
endif()